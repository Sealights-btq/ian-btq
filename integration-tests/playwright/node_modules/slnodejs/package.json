{
  "name": "slnodejs",
  "version": "6.1.1081",
  "description": "",
  "main": "tsOutputs/api.js",
  "workspaces": [
    "lean-coverage-agent"
  ],
  "scripts": {
    "install-browser-agent": "cd browser-agent && npm install",
    "install-lightweight-agent": "cd lightweight-agent && npm install",
    "install-node-agent": "npm install",
    "test": "npm run test:unit && npm run ct:build-scanner",
    "agent-test": "mocha 'tsOutputs/test/agent-tests/*-spec.js' -fi testcafe",
    "agent-test:testcafe": "mocha 'tsOutputs/test/agent-tests/*-spec.js' -f testcafe",
    "ut:common": "mocha \"tsOutputs/test/common/unitTest/**/*-spec.js\"",
    "ut:build-scanner": "mocha 'tsOutputs/test/build-scanner/unitTest/**/*-spec.js'",
    "ut:test-listener": "mocha \"tsOutputs/test/test-listener/unitTest/**/*-spec.js\"",
    "ut:cli-parse": "mocha \"tsOutputs/test/cli-parse/unitTest/**/*-spec.js\"",
    "ut:mocha-reporter": "mocha tsOutputs/test/mocha-reporter/reporter-spec.js",
    "ut:browser-agent": "cd browser-agent && npm test",
    "ut:lightweight-agent": "cd lightweight-agent && npm test",
    "ct:common": "mocha \"tsOutputs/test/common/componentTest/**/*-spec.js\"",
    "ct:build-scanner": "mocha \"tsOutputs/test/build-scanner/componentTest/**/*-spec.js\"",
    "test:unit": "mocha 'tsOutputs/test/*/unitTest/**/*-spec.js' && npm run ut:browser-agent && npm run ut:lightweight-agent",
    "build:node-agent": "tsc -p tsconfig.json && node ./copy-test-resources && npm run typings",
    "typings": "tsc -p tsconfig-typings.json",
    "build:browser-agent": "cd browser-agent && npm run build",
    "build:lightweight-agent": "cd lightweight-agent && npm run build",
    "build": "npm run lint && npm run build:node-agent && npm run build:browser-agent && npm run build:lightweight-agent",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "install-all": "npm run install-node-agent && npm run install-browser-agent && npm run install-lightweight-agent"
  },
